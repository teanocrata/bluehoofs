{
	"name": "bluehoofs",
	"version": "0.1.5",
	"private": true,
	"homepage": "https://teanocrata.github.io/bluehoofs",
	"bugs": {
		"url": "https://github.com/teanocrata/bluehoofs/issues"
	},
	"author": {
		"name": "teanocrata",
		"url": "https://teanocrata.github.io/"
	},
	"repository": {
		"repository": "github:teanocrata/bluehoofs",
		"type": "git"
	},
	"dependencies": {
		"@blueprintjs/core": "^3.30.1",
		"@types/node": "^12.0.0",
		"@types/react": "^16.9.0",
		"@types/react-dom": "^16.9.0",
		"@types/web-bluetooth": "^0.0.8",
		"mobx": "^5.15.5",
		"mobx-react": "^6.2.5",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet-async": "^1.0.9",
		"react-icons": "^4.2.0",
		"typescript": "~3.7.2",
		"web-vitals": "^1.1.1",
		"workbox-background-sync": "^5.1.3",
		"workbox-broadcast-update": "^5.1.3",
		"workbox-cacheable-response": "^5.1.3",
		"workbox-core": "^5.1.3",
		"workbox-expiration": "^5.1.3",
		"workbox-google-analytics": "^5.1.3",
		"workbox-navigation-preload": "^5.1.3",
		"workbox-precaching": "^5.1.3",
		"workbox-range-requests": "^5.1.3",
		"workbox-routing": "^5.1.3",
		"workbox-strategies": "^5.1.3",
		"workbox-streams": "^5.1.3"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write"
		]
	},
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"start": "react-scripts start",
		"build": "cross-env REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"git-cz": "git-cz",
		"release": "cross-env standard-version"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/runtime": "^7.13.10",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"@types/jest": "^24.0.0",
		"commitizen": "^4.1.2",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.2.0",
		"gh-pages": "^3.1.0",
		"lint-staged": "^10.2.11",
		"prettier": "2.2.1",
		"react-scripts": "3.4.0",
		"source-map-explorer": "^2.4.2",
		"standard-version": "^8.0.2"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}"
		]
	}
}
